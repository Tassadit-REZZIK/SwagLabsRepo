<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="sauce demo regression">
    <parameter name="useCloudEnv" value="false"></parameter>
    <parameter name="envName" value="browserstack"></parameter>
    <parameter name="osVersion" value="11"></parameter>
    <parameter name="browserVersion" value="108"></parameter>

    <parameter name="os" value="windows"></parameter>
    <parameter name="browserName" value="chrome"></parameter>
    <parameter name="url" value="https://www.saucedemo.com/"></parameter>
    <test name="login page">
            <classes>
                <class name="org.qaway.TestUI"></class>
                <class name="org.qaway.TestLogin"></class>
                <class name="org.qaway.TestPurchase"></class>
                <class name="org.qaway.TestFilter"></class>
                <class name="org.qaway.TestMenu"></class>
                <class name="org.qaway.TestValidatePrice"></class>
                <class name="org.qaway.TestScrollDownSwagLabs"></class>
                <class name="org.qaway.TestEndToEnd"></class>
                <class name="org.qaway.TestResetAppState"></class>
                <class name="org.qaway.TestLogout"></class>
                <class name="org.qaway.TestSearch"></class>
                <class name="org.qaway.TestLoginUsingDataBase"></class>
                <class name="org.qaway.TestContinueShopping"></class>
                <class name="org.qaway.TestRemoveItem"></class>
                <class name="org.qaway.TestHandleChatBot"></class>
                <class name="org.qaway.TestHandleAlert"></class>
                <class name="org.qaway.TestCustomers"></class>
            </classes>
    </test>
</suite>

